{
  "name": "@replit/clui",
  "version": "0.0.5",
  "description": "A CLI with UI",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "npm run test && npm run build",
    "typedoc:build": "typedoc src",
    "typedoc:serve": "npm run docs:build && npx serve docs",
    "docs:dev": "docz dev --host 0.0.0.0",
    "docs:build": "docz build",
    "docs:serve": "docz build && docz serve",
    "test": "npm-run-all --parallel test:*",
    "test:format": "prettier --check \"src/**/*.{js,json,ts,tsx}\"",
    "test:lint": "eslint src/ --ext .js,.ts,.tsx",
    "test:tsc": "tsc",
    "test:unit": "jest"
  },
  "author": "moudy@repl.it",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/faker": "^4.1.9",
    "@types/jest": "^24.9.0",
    "@types/react-dom": "^16.9.4",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "docz": "^2.2.0",
    "downshift": "^4.0.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "gatsby-plugin-styled-jsx": "^3.1.18",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-jsx": "^3.2.4",
    "ts-jest": "^24.3.0",
    "typedoc": "^0.16.7",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "arcsecond": "^2.0.3"
  }
}
